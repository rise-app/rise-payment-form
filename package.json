{
  "name": "@rise/payment-form",
  "repository": "https://github.com/rise-app/rise-payment-form",
  "version": "1.0.0-alpha.6",
  "main": "dist/index.min.js",
  "module": "dist/index.min.mjs",
  "scripts": {
    "build": "rollup -c",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "rollup -c -w",
    "lint": "eslint --color --ignore-path .gitignore .",
    "prepublishOnly": "npm run build",
    "start": "sirv public",
    "test": "start-server-and-test dev http://localhost:5000 cy:run",
    "stories": "start-storybook",
    "build-stories": "build-storybook",
    "static-stories": "build-storybook -c .storybook -o .out"
  },
  "description": "Web Component for RiSE Payments",
  "browserslist": [
    "defaults"
  ],
  "files": [
    "src",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "svelte"
  ],
  "license": "BSD",
  "lint-staged": {
    "*.{html, css, scss, stylus, js, ts, json, yml, md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js, svelte}": [
      "eslint --fix",
      "git add"
    ]
  },
  "svelte": "src/components/components.module.js",
  "dependencies": {
    "jsencrypt": "3.0.0-rc.1",
    "lodash.get": "4.4.2",
    "node-jsencrypt": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/svelte": "^5.3.19",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "@rollup/plugin-json": "4.0.2",
    "@testing-library/cypress": "5.0.2",
    "autoprefixer": "9.7.4",
    "cypress": "3.8.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-svelte3": "2.7.3",
    "husky": "4.2.1",
    "lint-staged": "10.0.2",
    "postcss": "7.0.26",
    "postcss-load-config": "2.1.0",
    "prettier": "2.0.5",
    "rollup": "2.22.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-svelte": "5.1.1",
    "rollup-plugin-terser": "6.1.0",
    "sirv-cli": "0.4.5",
    "start-server-and-test": "1.10.7",
    "svelte": "3.24.0",
    "svelte-check": "^0.1.55",
    "svelte-forms": "^1.2.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "3.3.0"
  }
}
